    <script>

        function notifyMe() {
            if (!("Notification" in window)) {
                alert("This Browser Not System");
            }
            else if (Notification.permission === "granted") {
                notify();
            }
            else if (Notification.permission !== 'denied') {
                Notification.requestPermission(function (permission) {
                    if (permission === "başarılı") {
                        notify();
                    }
                });
            }
            function notify() {
                var notification = new Notification('Bildirimleriniz Açık', {
                    icon: "aaa.jfif",
                    body: " Rd1ns Youtube Channel ",

                });
                notification.onclick = function () {
                    window.open("https://www.youtube.com/channel/UCpX0TAkHBqwASZSBA2lvAaA")
                };
                setTimeout(notification.close.bind(notification), 70000);
            }
        }
        notifyMe();

    </script>

